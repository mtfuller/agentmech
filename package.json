{
  "name": "@agentmech/agentmech",
  "version": "1.0.1",
  "description": "A Node.js CLI tool for running AI workflows locally with Ollama integration",
  "main": "dist/cli.js",
  "bin": {
    "agentmech": "./dist/cli.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc && npm run copy-views",
    "copy-views": "node scripts/copy-views.js",
    "start": "node dist/cli.js",
    "test": "npm run build && jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "workflow",
    "ollama",
    "cli",
    "automation",
    "llm",
    "rag",
    "mcp"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtfuller/agentmech.git"
  },
  "homepage": "https://github.com/mtfuller/agentmech#readme",
  "bugs": {
    "url": "https://github.com/mtfuller/agentmech/issues"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "docs/"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "@types/pdf-parse": "^1.1.5",
    "axios": "^1.13.2",
    "chalk": "^4.1.2",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "js-yaml": "^4.1.0",
    "mammoth": "^1.11.0",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.0",
    "jest": "^29.7.0",
    "jszip": "^3.10.1",
    "pdfkit": "^0.17.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.9.3"
  }
}
