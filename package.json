{
  "name": "agentmech",
  "version": "1.0.0",
  "description": "A Node.js CLI tool for running AI workflows locally with Ollama integration",
  "main": "dist/cli.js",
  "bin": {
    "agentmech": "./dist/cli.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc && npm run copy-views",
    "copy-views": "node scripts/copy-views.js",
    "start": "node dist/cli.js",
    "test": "npm run build && jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "workflow",
    "ollama",
    "cli",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "axios": "^1.13.2",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.9.3"
  }
}
